			<!-- getString("424324.xxxx") 获取到 一个map结构 -->
			<entry>
				<key>xxxx</key>
				<entrys>
					<!-- getString("424324.xxxx.Id2") 获取到 tttttSS -->
					<entry>
						<key>Id2</key>
						<value>tttttSS</value>
					</entry>
				</entrys>
			</entry>
		</entrys>
